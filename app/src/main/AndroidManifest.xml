<?xml version="1.0" encoding="utf-8"?>
<manifest
	package="com.nullcognition.androiddatabaseprogramming"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>

	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme"
		>
		<activity
			android:name="com.nullcognition.androiddatabaseprogramming.ActivityMain"
			android:label="@string/app_name"
			>
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
			android:name="com.nullcognition.androiddatabaseprogramming.ActivitySchema"
			android:label="@string/title_activity_activity_schema"
			>
		</activity>
	</application>

</manifest>
